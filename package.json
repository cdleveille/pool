{
  "name": "fullstack-ts",
  "version": "1.0.0",
  "main": "build/server/index.js",
  "repository": "https://github.com/cdleveille/fullstack-ts.git",
  "author": "Chris Leveille <cdleveille@gmail.com>",
  "description": "MERN stack monorepo template with types shared between server and client.",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx . && prettier --check .",
    "format": "eslint --fix --ext .js,.jsx,.ts,.tsx . && prettier --write .",
    "build": "rimraf build && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"yarn build:server\" \"yarn build:client\"",
    "build:server": "rimraf build/server && rimraf build/shared && tsc -b src/server",
    "build:client": "rimraf build/client && webpack",
    "dev": "concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"yarn dev:server\" \"yarn dev:client\"",
    "dev:server": "ts-node-dev --respawn --transpile-only --project src/server/tsconfig.json src/server/index.ts",
    "dev:client": "webpack watch",
    "start": "node ."
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "mongodb": "^4.7.0",
    "mongoose": "^6.4.4",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.2",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "javascript-obfuscator": "^4.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-obfuscator": "^3.5.1",
    "workbox-webpack-plugin": "^6.5.3"
  }
}
